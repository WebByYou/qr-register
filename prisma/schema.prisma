generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}



model Registration {
  id         Int      @id @default(autoincrement())
  firstName  String
  lastName   String
  employeeId String   @unique
  createdAt  DateTime @default(now())
}
